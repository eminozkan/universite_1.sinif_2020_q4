#include <iostream>

using namespace std;

int main(){
    const int numberofSpaces = 2;
    int startNumber;
    cout << "Enter a number:";
    cin >> startNumber;





    int result = 0;
    for(int k = 0; k<20; k++) {
        for (int i = 0; i < 10; i++) {
            result = result + startNumber;
            if(result<10) {
                cout << "   " << result;
            }
            else if(result < 100) {
                cout <<"  " << result;
            }
            else if(result < 1000) {
                cout <<" " << result;
            }else
                cout << result;
            for (int j = 0; j < numberofSpaces; j++) {
                cout << ' ';
            }
        }
        cout << endl;
    }


return 0;

}
